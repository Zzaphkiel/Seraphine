QLabel {
    color: white;
    font: 14px 'Segoe UI', 'Microsoft YaHei';
}

QLabel#name {
    color: white;
    font: bold 32px 'Segoe UI', 'Microsoft YaHei';
}

QLabel#tagLineLabel {
    font: 16px;
    color: rgb(200, 200, 200);
}

QLabel#rencent20GamesLabel,
QLabel#winsLabel,
QLabel#lossesLabel,
QLabel#kdaLabel,
QLabel#kdaValLabel {
    color: white;
    font: 14px 'Segoe UI', 'Microsoft YaHei';
}

QLabel#winsLabel {
    font: bold;
    color: rgb(57, 176, 27);
}

QLabel#lossesLabel {
    font: bold;
    color: rgb(211, 25, 12);
}

QLabel#totalLabel {
    font: bold;
    color: --ThemeColorPrimary;
}

TeammateInfoBar>#winsLabel {
    font: bold;
    color: rgb(57, 176, 27);
}

TeammateInfoBar>#lossesLabel {
    font: bold;
    color: rgb(211, 25, 12);
}

#gameInfoArea {
    border: 1px solid rgb(35, 35, 35);
    border-radius: 6px;
    background-color: rgba(255, 255, 255, 0.051);
}

#gameInfoWidget {
    background: transparent;
}

GameInfoBar {
    background-color: rgb(39, 39, 39);
}

GameInfoBar>QLabel {
    color: white;
    background: transparent;
    font: 14px 'Segoe UI', 'Microsoft YaHei';
}

ItemsKdaCsGold>QLabel {
    font: 16px 'Segoe UI', 'Microsoft YaHei';
}

CareerInterface {
    border: none;
    background-color: transparent;
}

TeammateInfoBar {
    border: 1px solid rgb(35, 35, 35);
    border-radius: 6px;
    background-color: rgba(255, 255, 255, 0.051);
}

TeammateInfoBar:hover {
    border: 1px solid rgba(0, 0, 0, 0.095);
    border-radius: 6px;
    background-color: rgba(255, 255, 255, 0.0837);
}

SummonerName {
    font: bold 14px 'Segoe UI', 'Microsoft YaHei';
    text-align: center;
    padding: 4px 6px 4px 6px;
    color: white;
    border: none;
    background-color: transparent;
}

SummonerName:hover {
    color: --ThemeColorDark1;
}

SummonerName[pressed=true] {
    color: --ThemeColorDark3;
}